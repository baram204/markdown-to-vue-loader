{
  "name": "markdown-to-vue-loader",
  "version": "0.2.0",
  "description": "Markdown to Vue component loader for Webpack.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "license": "MIT",
  "repository": "xkeshi/markdown-to-vue-loader",
  "homepage": "https://xkeshi.github.io/markdown-to-vue-loader",
  "author": {
    "name": "Chen Fengyuan",
    "url": "http://chenfengyuan.com"
  },
  "files": [
    "src",
    "dist/*.js"
  ],
  "keywords": [
    "markdown",
    "vue",
    "vue-component",
    "vue.js",
    "webpack-loader",
    "front-end"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:docs": "cross-env NODE_ENV=production webpack",
    "clear": "rimraf dist",
    "clear:docs": "rimraf docs/dist docs/index.html",
    "deploy": "npm run lint:docs && npm run clear:docs && npm run build:docs",
    "dev": "npm run clear:docs && cross-env NODE_ENV=development webpack-dev-server --open",
    "lint": "eslint src test *.config.js --ext .js,.vue --fix",
    "lint:docs": "eslint docs/src --ext .js,.vue --fix",
    "prebuild": "npm run lint && npm run clear",
    "release": "npm run build && npm test && npm run deploy",
    "start": "npm run dev",
    "test": "mocha test/index.js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.4.0"
  },
  "peerDependencies": {
    "vue-loader": "^13.0.0",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "html-webpack-plugin": "^2.30.1",
    "ip": "^1.1.5",
    "memory-fs": "^0.4.1",
    "mocha": "^4.0.1",
    "postcss": "^6.0.13",
    "postcss-atrule-bem": "^3.0.1",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "postcss-smart-import": "^0.7.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "style-loader": "^0.19.0",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0"
  }
}
