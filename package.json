{
  "name": "markdown-to-vue-loader",
  "version": "0.1.0",
  "description": "Markdown to Vue component loader for Webpack.",
  "main": "dist/index.js",
  "module": "dist/esm.js",
  "license": "MIT",
  "repository": "xkeshi/markdown-to-vue-loader",
  "homepage": "https://xkeshi.github.io/markdown-to-vue-loader",
  "author": {
    "name": "Chen Fengyuan",
    "email": "chenfengyuan@outlook.com",
    "url": "http://chenfengyuan.com"
  },
  "keywords": [
    "markdown",
    "vue",
    "vue-component",
    "vue.js",
    "webpack-loader",
    "front-end"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:docs": "cross-env NODE_ENV=production webpack",
    "clear": "rimraf dist",
    "clear:docs": "rimraf docs/dist docs/index.html",
    "deploy": "npm run lint:docs && npm run clear:docs && npm run build:docs",
    "dev": "npm run clear:docs && cross-env NODE_ENV=development webpack-dev-server --open",
    "lint": "eslint src test *.config.js --ext .js,.vue --fix",
    "lint:docs": "eslint docs/src --ext .js,.vue --fix",
    "postbuild": "npm run test && npm run deploy",
    "prebuild": "npm run lint && npm run clear",
    "start": "npm run dev",
    "test": "mocha test/index.js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.4.0"
  },
  "peerDependencies": {
    "vue-loader": "^13.0.0",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "ip": "^1.1.5",
    "memory-fs": "^0.4.1",
    "mocha": "^3.5.3",
    "postcss": "^6.0.11",
    "postcss-atrule-bem": "^3.0.1",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "style-loader": "^0.18.2",
    "vue": "^2.4.4",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  }
}
